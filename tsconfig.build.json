{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "skipLibCheck": true,
        "skipDefaultLibCheck": true,
        "noEmitOnError": false,
        "emitDeclarationOnly": false,
        "noImplicitAny": false,
        "strictNullChecks": false,
        "strictPropertyInitialization": false,
        "strictFunctionTypes": false,
        "strictBindCallApply": false,
        "alwaysStrict": false,
        "strict": false,
        "allowJs": true,
        "checkJs": false
    },
    "exclude": [
        "node_modules",
        "**/*.spec.ts",
        "**/*.test.ts"
    ]
}
